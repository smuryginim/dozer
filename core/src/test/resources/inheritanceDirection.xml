<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">

  <mapping wildcard="true">
    <class-a>org.dozer.vo.direction.ContentItemGroupBase</class-a>
    <class-b>org.dozer.vo.direction.ContentItemGroupDTO</class-b>
    <field>
      <a>childGroups</a>
      <b>childGroups</b>
      <a-hint>org.dozer.vo.direction.ContentItemGroupDefault</a-hint>
      <b-hint>org.dozer.vo.direction.ContentItemGroupDTO</b-hint>
    </field>
  </mapping>


  <mapping wildcard="false">
    <class-a>org.dozer.vo.direction.EntityBase</class-a>
    <class-b>org.dozer.vo.direction.EntityDTO</class-b>
    <field>
      <a is-accessible="true">id</a>
      <b is-accessible="true">id</b>
    </field>
  </mapping>

</mappings>